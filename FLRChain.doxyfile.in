DOXYFILE_ENCODING = UTF-8

PROJECT_NAME = FLRChain
PROJECT_NUMBER = $$VERSION
PROJECT_BRIEF = \"Blockchain for forest landscape restoration\"
PROJECT_LOGO = doc/img/logo.svg

INPUT = flrchain/src
FILE_PATTERNS = *.h *.cpp *.md *.txt

MARKDOWN_SUPPORT = YES
TOC_INCLUDE_HEADINGS = 99

RECURSIVE = TRUE
QT_AUTOBRIEF = YES

EXTRACT_ALL = YES
EXTRACT_PRIVATE = YES
EXTRACT_PRIV_VIRTUAL = YES
EXTRACT_PACKAGE = YES
EXTRACT_STATIC = YES
EXTRACT_LOCAL_CLASSES = YES

WARNINGS = YES
WARN_AS_ERROR = NO
WARN_IF_UNDOCUMENTED = YES
WARN_IF_DOC_ERROR = YES
WARN_NO_PARAMDOC = NO

SOURCE_BROWSER = YES

DISTRIBUTE_GROUP_DOC = YES

GENERATE_TREEVIEW = YES
GENERATE_HTML = YES
GENERATE_TAGFILE = NO
GENERATE_LATEX = NO
GENERATE_RTF = NO
GENERATE_MAN = NO
GENERATE_XML = NO

HTML_OUTPUT = doc/html/
HTML_FILE_EXTENSION = .html
HTML_HEADER = doc/branding/header.html
HTML_FOOTER = doc/branding/footer.html
HTML_EXTRA_STYLESHEET = doc/branding/doxygen-awesome.css \
                        doc/branding/doxygen-awesome-sidebar-only.css \
                        doc/branding/doxygen-awesome-sidebar-only-darkmode-toggle.css
HTML_EXTRA_FILES = doc/branding/doxygen-awesome-darkmode-toggle.js
HTML_DYNAMIC_SECTIONS = YES
IMAGE_PATH = img/

TAB_SIZE = 4
INHERIT_DOCS = YES
BUILTIN_STL_SUPPORT = YES
GENERATE_TODOLIST = YES
FULL_PATH_NAMES = YES
STRIP_FROM_PATH = .

ALLEXTERNALS = YES
EXTERNAL_GROUPS = YES
EXTERNAL_PAGES = YES

HAVE_DOT = YES
CALL_GRAPH = YES
CALLER_GRAPH = YES
